<Window x:Class="Coupon_Generator.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AboutWindow" Height="125" Width="300">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Top" Orientation="Vertical">
                <TextBlock HorizontalAlignment="Center"  Text="This program was made by" />
                <TextBlock HorizontalAlignment="Center"  Text="Andrew Godfroy of Killerrin Studios"/>
            </StackPanel>
            
            <Button Content="Ok" DockPanel.Dock="Bottom" Margin="0,20,0,0" Click="OKButton_Click"/>
        </DockPanel>
    </Grid>
</Window>
